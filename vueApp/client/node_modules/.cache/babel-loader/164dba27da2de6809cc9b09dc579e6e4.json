\n    },\r\n    saveClose() {\r\n      this.$store.Item.lastEdited = new Date()\r\n      axios.patch(`/lists/${this.$store.Item._id}`, this.$store.Item)\r\n        .then(() => {\r\n          this.$modal.hide('writeListModal')\r\n        })\r\n    },\r\n    hide() {\r\n      this.$modal.hide('writeListModal')\r\n    },\r\n    saveChanges() {\r\n      axios.patch(`/lists/${this.$store.Item._id}`, this.$store.Item)\r\n    },\r\n  },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/modals"}]}